<li class="col s12 m6 l4">
	<h6 style="font-weight:bolder">{{name}}</h6><br/><br/>
	<button class="col s12 green darken-2 change-eaten btn z-depth-4" style="height:60px;" data-id="{{id}}" data-newEaten="{{eaten}}">
		{{#if eaten}}EAT!{{else}}SECONDS!{{/if}}
		<i class="large material-icons">fastfood</i>
	</button>
	<button class="col s12 red darken-4 delete-burger btn z-depth-4" style="height:40px;" data-id="{{this.id}}">
		<i class="large material-icons">delete_forever</i>
	</button>
	<br/><br/><br/>
</li>
